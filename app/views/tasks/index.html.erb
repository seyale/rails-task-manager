<h1>TASKS</h1>


<table>
  <th>Task</th>
  <th>Details</th>
  <th>Status</th>
  <tbody>
  <% @tasks.each do |task| %>
    <tr>
      <td><%= link_to task.title, task_path(task) %></td>
      <td><%= task.details %></td>
      <td><%= task.completed %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to 'Add new task', new_task_path %>
